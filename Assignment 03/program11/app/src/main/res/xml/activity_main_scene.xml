<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="55dp"
            android:layout_width="177dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toStartOf="@+id/btnUpdate"
            motion:layout_constraintHorizontal_bias="0.022" />
        <Constraint
            android:layout_height="65dp"
            android:layout_width="190dp"
            android:id="@+id/btnUpdate"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="1.0" />
        <Constraint
            android:layout_height="65dp"
            android:layout_width="190dp"
            android:id="@+id/btnDelete"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.609"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.004" />
        <Constraint
            android:layout_height="65dp"
            android:layout_width="190dp"
            android:id="@+id/btnView"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.612"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="1.0" />
        <Constraint
            android:layout_width="177dp"
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="338dp" />
        <Constraint
            android:layout_width="177dp"
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="338dp" />
        <Constraint
            android:layout_width="177dp"
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="338dp" />
        <Constraint
            android:layout_width="190dp"
            android:layout_height="65dp"
            android:id="@+id/btnInsert"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>